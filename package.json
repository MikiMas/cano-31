{
  "name": "canoo-monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "api:dev": "npm --prefix apps/api run dev",
    "api:build": "npm --prefix apps/api run build",
    "api:start": "npm --prefix apps/api run start",
    "api:lint": "npm --prefix apps/api run lint",
    "web:dev": "npm --prefix apps/web run dev",
    "web:build": "npm --prefix apps/web run build",
    "web:start": "npm --prefix apps/web run start",
    "web:lint": "npm --prefix apps/web run lint",
    "mobile:dev": "npm --prefix apps/mobile run start",
    "mobile:android": "npm --prefix apps/mobile run android",
    "mobile:ios": "npm --prefix apps/mobile run ios",
    "mobile:web": "npm --prefix apps/mobile run web"
  },
  "dependencies": {
    "expo-media-library": "~18.2.1",
    "expo-screen-orientation": "~9.0.8"
  }
}
